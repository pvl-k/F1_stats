# Build
docker build -t pipeline-app .
# First start
docker run --name=pipeline-app-container -d -p 5432:5432 -e POSTGRES_PASSWORD=postgres pipeline-app
# Next starts
docker start pipeline-app-container
# Stop 
docker stop pipeline-app-container
